<table id="quotasContent" style="width: 100%;">
  <thead>
    <tr>
      <td>№</td>
      <td>Фамилия</td>
      <td>Имя</td>
      <td>Отчество</td>
      <td>Должность</td>
      <td>Квота</td>
      <td>Доступно</td>
      <td>Описание</td>
    </tr>
  </thead>
  <tbody>
    <% @quotum_doctors.each do |quotum_doctor| %>
      <tr>
        <td><div class="label"><%= quotum_doctor.doctor.id %></div></td>
        <td><div class="label"><%= quotum_doctor.doctor.surname %></div></td>
        <td><div class="label"><%= quotum_doctor.doctor.name %></div></td>
        <td><div class="label"><%= quotum_doctor.doctor.patronymic %></div></td>
        <td><div class="label"><%= quotum_doctor.post.name %></div></td>
        <td><input class="textBox" placeholder="Квота" type="text" name="quota" style="width:200px;" value="<%= quotum_doctor.full %>" form="data" /></td>
        <td>
          <span class="checkBox">
            <% if quotum_doctor.status.id == 1 %>
              <input type="checkbox" name="active" checked="" tabindex="0" form="data" />
            <% else %>
              <input type="checkbox" name="active" tabindex="0" form="data" />
            <% end %>
          </span>
        </td>
        <td><input class="textBox" placeholder="Описание" type="text" name="description" style="width:200px;" value="<%= quotum_doctor.description %>" form="data" /></td>
      </tr>
    <% end %>
  </tbody>
</table>
